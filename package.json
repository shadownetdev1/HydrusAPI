{
  "name": "hydrusapi",
  "version": "80.638.6",
  "description": "A JavaScript based asynchronous implementation of the Hydrus client API with documentation, tests, and type definitions.",
  "homepage": "https://github.com/shadownetdev1/HydrusAPI",
  "bugs": {
    "url": "https://github.com/shadownetdev1/HydrusAPI/issues"
  },
  "repository": "github:shadownetdev1/HydrusAPI",
  "main": "hydrusapi.js",
  "scripts": {
    "test": "cp scripts/pre-commit .git/hooks/pre-commit && vitest run --coverage --coverage.reporter=text --coverage.reportsDirectory=/tmp/hydrusapi_coverage",
    "test:watch": "cp scripts/pre-commit .git/hooks/pre-commit && vitest --coverage --coverage.reporter=text --coverage.reportsDirectory=/tmp/hydrusapi_coverage"
  },
  "keywords": [
    "hydrusnetwork",
    "hydrus",
    "hydrus-network"
  ],
  "files": [
    "types/",
    "CHANGELOG.MD",
    "hydrusapi.js",
    "license.txt",
    "README.MD"
  ],
  "license": "SEE LICENSE IN license.txt",
  "type": "commonjs",
  "cspell": {
    "version": "0.2",
    "language": "en",
    "words": [
      "APNG",
      "azur",
      "booru",
      "Hodan",
      "hpos",
      "hydrusapi",
      "IPFS",
      "shadownetdev1",
      "Ugoira",
      "Ugoiras",
      "unanchor",
      "unzstd",
      "vpos"
    ],
    "ignoreWords": [
      "someoverbooru"
    ],
    "flagWords": [],
    "allowCompoundWords": true,
    "dictionaries": [
      "en_US",
      "en-gb",
      "companies",
      "softwareTerms",
      "misc",
      "typescript",
      "npm"
    ]
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@types/node": "^24.2.0",
    "@vitest/coverage-v8": "^3.2.4",
    "deep-object-diff": "^1.1.9",
    "eslint": "^9.32.0",
    "fs-jetpack": "^5.1.0",
    "globals": "^16.3.0",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "devEngines": {
    "os": {
      "name": "linux",
      "onFail": "error"
    }
  }
}
